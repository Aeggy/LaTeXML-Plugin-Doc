      </w:pPr>
      <w:r>
        <w:rPr>
          <w:b/>
        </w:rPr>
        <w:t>Exponential speed of uniform convergence of the cell density toward equilibrium for subcritical mass in a Patlak-Keller-Segel model </w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:pStyle w:val="style36"/>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:b/>
        </w:rPr>
        <w:t>Alexandre MONTARU</w:t>
      </w:r>
